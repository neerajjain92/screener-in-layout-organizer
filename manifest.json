{
  "manifest_version": 3,
  "name": "Screener Layout Modifier",
  "version": "1.0",
  "description": "Modifies Screener grid layout and company profile width",
  "permissions": ["activeTab", "storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
